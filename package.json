{
    "name": "edurange3",
    "version": "1.0.0",
    "description": "EDURange",
    "scripts": {
      "build": "NODE_ENV=production webpack --progress --colors -p && npm run flask-static-digest",
      "start": "concurrently -n \"FLASK,CELERY,FLOWER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgRed.bold,bgCyan.bold,bgYellow.bold\" \"npm run flask-server\" \"celery -A py_flask.utils.tasks worker -B -E -f celery.log -l DEBUG\" \"celery -A py_flask.utils.tasks --broker=redis://localhost:6379/0 flower --port=5555\" ",
      "flask-server": "flask run --host=0.0.0.0",
      "flask-static-digest": "flask digest compile",
      "lint": "eslint \"assets/js/*.js\""
    },
    "repository": {
      "type": "git",
      "url": "git+https://https://github.com/edurange/edurange3.git"
    },
    "author": "Jack Cook",
    "license": "MIT",
    "engines": {
      "node": ">=14"
    },
    "browser": {
      "fs": false
    },
    "bugs": {
      "url": "https://github.com/coojac09/edurange_refactored/issues"
    },
    "homepage": "https://github.com/coojac09/edurange_refactored#readme",
    "dependencies": {
      "cidr-matcher": "^2.1.1",
      "cors": "^2.8.5",
      "dotenv": "^16.0.2",
      "fs": "^0.0.1-security",
      "jsx": "^0.9.89",
      "npm": "^8.5.1",
      "path": "^0.12.7",
      "redis": "^3.0.2",
      "serve-favicon": "^2.5.0",
      "validator": "^13.5.2"
    },
    "devDependencies": {
      "@babel/core": "^7.12.3",
      "@babel/helper-call-delegate": "^7.12.1",
      "@babel/preset-env": "^7.18.6",
      "@babel/preset-react": "^7.18.6",
      "@json-editor/json-editor": "^2.5.1",
      "babel-eslint": "^10.1.0",
      "babel-loader": "^8.2.1",
      "concurrently": "^5.2.0",
      "css-loader": "^3.5.3",
      "eslint": "^6.2.2",
      "eslint-config-airbnb-base": "^14.2.1",
      "eslint-plugin-import": "^2.22.1",
      "expose-loader": "^1.0.1",
      "file-loader": "^5.1.0",
      "install": "^0.13.0",
      "less": "^3.9.0",
      "less-loader": "^5.0.0",
      "mini-css-extract-plugin": "^0.9.0",
      "nodemon": "^2.0.19",
      "raw-loader": "^4.0.2",
      "read-config-ng": "^3.0.7",
      "url-loader": "^3.0.0"
    }
  }
  